<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Voter">
        <activity android:name=".Modify"
            android:label="Modify Record"
            android:parentActivityName=".MainActivity2"></activity>
        <activity android:name=".Search"
            android:label="Search Record"
            android:parentActivityName=".MainActivity2"/>
        <activity android:name=".Display"
            android:label="Delete Record"
            android:parentActivityName=".MainActivity2"/>
        <activity android:name=".Insert"
            android:label="Insert Record"
            android:parentActivityName=".MainActivity2"/>
        <activity android:name=".MainActivity2"
            android:label="Perform Action"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>